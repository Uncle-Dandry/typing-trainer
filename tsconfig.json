{
  "compilerOptions": {
    "baseUrl": "src",
    "target": "es6",
    "lib": [
      "DOM",
      "ESNext",
    ],
    "jsx": "react-jsx",
    "module": "ESNext",
    "moduleResolution": "Node",
    "types": [
      "@types/react",
      "@types/react-dom",
      "react",
      "react-dom"
    ],
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
